<Page x:Class="ПР6.PageAdminMenu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ПР6"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageAdminMenu">

    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <Button Name="btnSelectUser" Content="Пользователи" Click="btnSelectUser_Click"  Template="{StaticResource CTButon3}"/>
                <Button Name="btnSelectTovar" Content="Товары" Click="btnSelectTovar_Click" Template="{StaticResource CTButon3}"/>
                <Button Name="btnLC" Content="Личный кабинет" Click="btnLC_Click" Template="{StaticResource CTButon3}"/>
            </StackPanel>
        </StackPanel>
        
    </Grid>
</Page>
