<Window x:Class="ПР6.page.WindowUpdateLodinPassword"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ПР6.page"
        mc:Ignorable="d"
        Title="WindowUpdateLodinPassword" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <StackPanel>
                <GroupBox Header="Новый логин">
                    <TextBox Name="Login"/>
                </GroupBox>
                <GroupBox Header="Новый пароль">
                    <PasswordBox Name="Password"/>
                </GroupBox>
            </StackPanel>
            <StackPanel>
                <Button Name="btnSave" Content="Сохранить" Click="btnSave_Click"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
